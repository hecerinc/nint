function sort(int[] a) :: void {
	int i = 0;
	int j = 0;
	int n = len(a);
	while(i < n-1) {
		while(j < n-i-1) {
			if(a[j] > a[j+1]) {
				int temp = a[j];
				a[j] = a[j+1];
				a[j+1] = temp;
			}
			j = j + 1;
		}
		i = i + 1;
	}
}
int[] a = [5,2,3,48,12];

print(a);
sort(a);
print(a);
